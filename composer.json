{
    "name": "polass/view-model",
    "description": "ViewModel for Laravel",
    "type": "library",
    "homepage": "https://github.com/polass/view-model",
    "keywords": [ "laravel" ],
    "authors": [
        {
            "name": "Salop, Inc.",
            "homepage": "http://www.salop.co.jp/"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": " ~5.1",
        "polass/fluent": "^1.1",
        "polass/enum": "^1.0",
        "illuminate/contracts": " ~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Polass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polass\\Tests\\": "tests/"
        }
    },
    "license": "MIT"
}
